{
  $schema: 'https://docs.renovatebot.com/renovate-schema.json',
  configMigration: true,
  forkProcessing: "enabled",
  extends: [
    'config:recommended',
    ':disableRateLimiting',
    ':separateMultipleMajorReleases',
  ],
  postUpdateOptions: [
    "gomodTidy",
  ],
  packageRules: [
    {
      matchManagers: [
        'gomod',
      ],
      groupName: 'go dependencies',
    },
  ],
}
